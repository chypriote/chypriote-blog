<header class="app-navbar">
	<img class="avatar" src="<%- url_for(theme.avatar) %>" alt="avatar" />
	<h1 class="title">
		<a class="hover-effect" href="/" title="<%- theme.title %>" data-hover="<%- theme.title %>"><%- theme.title %></a>
	</h1>
	<nav class="app-nav">
		<% var first = true
			for (var i in theme.nav){ %>
				<% if (is_current(i)) { %>
					<a class="active" href="<%- url_for(theme.nav[i]) %>"><%= i %></a>
				<% } else { %>
					<a href="<%- url_for(theme.nav[i]) %>"><%= i %></a>
				<% } %>
		<% } %>
	</nav>
</header>
