<header class="app-header">
	<section class="banner">
		<div class="placeholder"></div>
	</section>
	<nav class="app-nav">
		<% var first = true
			for (var i in theme.nav){ %>
				<% if (first) { %>
					<a class="active" href="<%- url_for(theme.nav[i]) %>"><%= i %></a>
					<% first = false %>

				<% } else { %>
					<a href="<%- url_for(theme.nav[i]) %>"><%= i %></a>
				<% } %>
		<% } %>
	</nav>

	<section class="description">
		<img class="avatar" src="<%- url_for(theme.avatar) %>" alt="avatar" />
		<h1 class="title"> <%- theme.title %> </h1>
		<h3 class="subtitle"> <%- theme.subtitle %> </h3>
	</section>
</header>
